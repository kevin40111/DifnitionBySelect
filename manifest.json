{
  "name": "Easy Word Dictionary",
  "description": "select word get definition immediately",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
        "matches": ["https://*/*", "http://*/*"],
        "js": ["content.js"]
    }
 ]
}
